
// vertex shader of glsl
#version 330

layout (location = 0) in vec3 Position;
layout (location = 1) in vec4 Color;
layout (location = 2) in vec2 TextureUV;

out VS_OUTPUT
{
    vec4 Color;
    vec2 TextureUV;
} vs_out;


void main(void) 
{
   gl_Position = vec4(Position,1);
   vs_out.Color = Color;
   vs_out.TextureUV = TextureUV;
}